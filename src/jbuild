(library
 ((name        magic_mime_library)
  (public_name magic-mime)
  (wrapped     false)
))

(rule
 ((targets (mime_types.ml))
  (deps    (../generator/generate_mime_types.exe))
  (action  (with-stdout-to ${@}
             (progn (run ${<} ../../../mime.types)
                    (run ${<} ../../../more-mime.types --files))))))
